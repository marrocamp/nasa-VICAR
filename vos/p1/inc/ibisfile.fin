      CHARACTER*4 ICOLUMN_SIZE
      PARAMETER(ICOLUMN_SIZE='SIZE') 

! record properties 
      CHARACTER*8 IRECORD_U_FORMAT
      PARAMETER(IRECORD_U_FORMAT='U_FORMAT') 
      CHARACTER*6 IRECORD_U_SIZE
