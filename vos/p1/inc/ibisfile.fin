      CHARACTER*2 IINIT_ON
      PARAMETER(IINIT_ON='ON') 		
      CHARACTER*3 IINIT_OFF
      PARAMETER(IINIT_OFF='OFF') 		

! column properties 
      CHARACTER*8 ICOLUMN_U_FORMAT
