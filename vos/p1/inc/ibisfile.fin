      CHARACTER*5 ICLOSE_UKEEP
      PARAMETER(ICLOSE_UKEEP='UKEEP') 
      CHARACTER*7 ICLOSE_UDELETE
      PARAMETER(ICLOSE_UDELETE='UDELETE') 	

! IBIS_GROUP modify options 
      CHARACTER*6 IGROUP_APPEND
      PARAMETER(IGROUP_APPEND='APPEND') 
      CHARACTER*6 IGROUP_INSERT
      PARAMETER(IGROUP_INSERT='INSERT') 
      CHARACTER*6 IGROUP_REMOVE
      PARAMETER(IGROUP_REMOVE='REMOVE') 

! IBIS_FILE organizations 
      CHARACTER*3 IORG_ROW
      PARAMETER(IORG_ROW='ROW') 
      CHARACTER*6 IORG_COLUMN
      PARAMETER(IORG_COLUMN='COLUMN') 

! group types 
      CHARACTER*6 ITYPE_FORMAT
      PARAMETER(ITYPE_FORMAT='FORMAT') 
      CHARACTER*4 ITYPE_UNIT
      PARAMETER(ITYPE_UNIT='UNIT') 
      CHARACTER*5 ITYPE_GROUP
      PARAMETER(ITYPE_GROUP='GROUP') 
      CHARACTER*5 ITYPE_LOCAL
      PARAMETER(ITYPE_LOCAL='LOCAL') 
      CHARACTER*3 ITYPE_ANY
      PARAMETER(ITYPE_ANY='ANY') 

