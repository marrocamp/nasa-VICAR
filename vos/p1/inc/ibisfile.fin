      PARAMETER(IFILE_NC='NC') 
      CHARACTER*3 IFILE_ORG
      PARAMETER(IFILE_ORG='ORG') 
      CHARACTER*4 IFILE_MODE
      PARAMETER(IFILE_MODE='MODE') 
      CHARACTER*7 IFILE_FORMATS
      PARAMETER(IFILE_FORMATS='FORMATS') 
      CHARACTER*5 IFILE_UNITS
      PARAMETER(IFILE_UNITS='UNITS') 
      CHARACTER*6 IFILE_GROUPS
      PARAMETER(IFILE_GROUPS='GROUPS') 
      CHARACTER*6 IFILE_LOCALS
      PARAMETER(IFILE_LOCALS='LOCALS') 
      CHARACTER*5 IFILE_VUNIT
      PARAMETER(IFILE_VUNIT='VUNIT') 
      CHARACTER*6 IFILE_U_SIZE
      PARAMETER(IFILE_U_SIZE='U_SIZE') 
