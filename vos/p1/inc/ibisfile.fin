      PARAMETER(IFILE_AUTO_INIT='AUTO_INIT') 
      CHARACTER*6 IFILE_PIX_NL
      PARAMETER(IFILE_PIX_NL='PIX_NL') 
      CHARACTER*6 IFILE_PIX_NS
