      CHARACTER*7 IRECORD_COLUMNS
      PARAMETER(IRECORD_COLUMNS='COLUMNS') 
      CHARACTER*5 IRECORD_IUNIT
      PARAMETER(IRECORD_IUNIT='IUNIT') 
      CHARACTER*2 IRECORD_NR
      PARAMETER(IRECORD_NR='NR') 		  
      CHARACTER*2 IRECORD_NC
      PARAMETER(IRECORD_NC='NC') 		  
      CHARACTER*3 IRECORD_ROW
      PARAMETER(IRECORD_ROW= 'ROW') 		 
      CHARACTER*8 IRECORD_REC_SIZE
      PARAMETER(IRECORD_REC_SIZE='REC_SIZE') 

! formats 
      CHARACTER*4 IFMT_BYTE
      PARAMETER(IFMT_BYTE='BYTE') 
      CHARACTER*4 IFMT_HALF
      PARAMETER(IFMT_HALF='HALF') 
      CHARACTER*4 IFMT_FULL
      PARAMETER(IFMT_FULL='FULL') 
      CHARACTER*4 IFMT_REAL
      PARAMETER(IFMT_REAL='REAL') 
      CHARACTER*4 IFMT_DOUB
      PARAMETER(IFMT_DOUB='DOUB') 
      CHARACTER*4 IFMT_COMP
      PARAMETER(IFMT_COMP='COMP') 
      CHARACTER*5 IFMT_ASCII
