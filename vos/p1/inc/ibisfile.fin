C
C ibisfile.fin -- Definition of all symbolic names for IBIS-2 internal
C symbols.  This module is used by application programs to define
C symbolic names for the IBIS-2 subroutines.  The values are the same
C as those used internally by IBIS-2.
C
C  Written By: Niles Ritter
C
      INTEGER UNDEFINED_OPTIONAL
      PARAMETER (UNDEFINED_OPTIONAL = -2)


! Array size definitions (Added per FR#85751) String limits include NULL 
      INTEGER I2_MAX_COL
      PARAMETER(I2_MAX_COL=1024)  ! Current somewhat arbitrary Max # columns   
      INTEGER I2_MAX_COL_OLD
      PARAMETER(I2_MAX_COL_OLD=100)   ! Limit for IBIS-1 files                     
      INTEGER I2_MAX_GRP_NAME
      PARAMETER(I2_MAX_GRP_NAME=32)    ! Maximum Group name string (no NULL)                 
      INTEGER I2_MAX_TYPE_NAME
      PARAMETER(I2_MAX_TYPE_NAME=3)     ! Maximum Type name string  (no NULL)         
      INTEGER I2_MAX_VALUE_NAME
      PARAMETER(I2_MAX_VALUE_NAME=16)    ! Maximum Value name string for IBIS Get/set 

! Size of input format string to IBIS_FILE_OPEN 
      INTEGER IFMT_SIZE
      PARAMETER(IFMT_SIZE=6) 

! IBIS_FILE  modes 
      CHARACTER*4 IMODE_READ
      PARAMETER(IMODE_READ='READ') 
      CHARACTER*5 IMODE_WRITE
      PARAMETER(IMODE_WRITE='WRITE') 
      CHARACTER*6 IMODE_OWRITE
      PARAMETER(IMODE_OWRITE='OWRITE') 
      CHARACTER*6 IMODE_UPDATE
      PARAMETER(IMODE_UPDATE='UPDATE') 

